<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
       xmlns:dailyFont="http://schemas.android.com/apk/res-auto">

    <com.daily.base.widget.DailyTextView
        android:id="@+id/titleTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:drawablePadding="4dp"
        android:gravity="center_horizontal"
        android:textColor="@color/default_text_c929292"
        android:textSize="12dp"/>

    <com.daily.base.widget.DailyTextView
        android:id="@+id/descriptionTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/titleTextView"
        android:layout_marginTop="4dp"
        android:gravity="center_horizontal"
        android:textColor="@color/default_text_c4d4d4d"
        android:textSize="17dp"/>

    <com.daily.base.widget.DailyTextView
        android:id="@+id/resultTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/descriptionTextView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="12dp"
        android:background="@drawable/selector_review_resultlayout"
        android:gravity="center"
        android:paddingBottom="5dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingTop="5dp"
        android:text="@string/label_review_score_default"
        android:textColor="@color/white"
        android:textSize="13dp"
        dailyFont:style="Medium"/>

    <RelativeLayout
        android:id="@+id/emoticonLayout"
        android:layout_width="wrap_content"
        android:layout_height="66dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:gravity="bottom|center_horizontal">

        <com.twoheart.dailyhotel.widget.DailyEmoticonImageView
            android:id="@+id/emoticonImageView0"
            android:layout_width="66dp"
            android:layout_height="66dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="-8dp"
            android:layout_marginRight="-8dp"/>

        <com.twoheart.dailyhotel.widget.DailyEmoticonImageView
            android:id="@+id/emoticonImageView1"
            android:layout_width="66dp"
            android:layout_height="66dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="-8dp"
            android:layout_marginRight="-8dp"
            android:layout_toRightOf="@+id/emoticonImageView0"/>

        <com.twoheart.dailyhotel.widget.DailyEmoticonImageView
            android:id="@+id/emoticonImageView2"
            android:layout_width="66dp"
            android:layout_height="66dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="-8dp"
            android:layout_marginRight="-8dp"
            android:layout_toRightOf="@+id/emoticonImageView1"/>

        <com.twoheart.dailyhotel.widget.DailyEmoticonImageView
            android:id="@+id/emoticonImageView3"
            android:layout_width="66dp"
            android:layout_height="66dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="-8dp"
            android:layout_marginRight="-8dp"
            android:layout_toRightOf="@+id/emoticonImageView2"/>

        <com.twoheart.dailyhotel.widget.DailyEmoticonImageView
            android:id="@+id/emoticonImageView4"
            android:layout_width="66dp"
            android:layout_height="66dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="-8dp"
            android:layout_marginRight="-8dp"
            android:layout_toRightOf="@+id/emoticonImageView3"/>
    </RelativeLayout>

    <View
        android:id="@+id/reviewScoreScrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</merge>