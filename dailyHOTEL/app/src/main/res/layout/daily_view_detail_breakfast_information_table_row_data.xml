<?xml version="1.0" encoding="utf-8"?>

<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.daily.base.widget.DailyTextView
            android:id="@+id/leftTextView"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:background="@color/dialog_inputmobile_line"
            android:freezesText="true"
            android:gravity="center"
            android:text="유아 (0~3세 이하)"
            android:textColor="@color/default_text_c929292"
            android:textSize="14dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <View
            android:layout_width="1dp"
            android:layout_height="40dp"
            android:background="@color/default_text_c323232"/>

        <com.daily.base.widget.DailyTextView
            android:id="@+id/rightTextView"
            android:layout_height="40dp"
            android:background="@color/white"
            android:freezesText="true"
            android:gravity="center"
            android:text="무료"
            android:textColor="@color/default_text_c323232"
            android:textSize="14dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
    </TableRow>
</layout>