<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="dailyFont">
        <attr name="style" format="enum">
            <enum name="Bold" value="0"/>
            <enum name="DemiLight" value="1"/>
            <enum name="Medium" value="2"/>
            <enum name="Regular" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="app">
        <attr name="drawableCompatLeft" format="reference"/>
        <attr name="drawableCompatTop" format="reference"/>
        <attr name="drawableCompatRight" format="reference"/>
        <attr name="drawableCompatBottom" format="reference"/>
        <attr name="maxHeight" format="dimension"/>
        <attr name="drawableCompatLeftFixedFirstLine" format="boolean"/>
    </declare-styleable>
</resources>